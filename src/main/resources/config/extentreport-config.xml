<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
  <configuration>
    <!-- 加载样式访问的需翻墙的外部资源地址，设置为extentreports则样式加载没问题 -->
    <cdn>extentreports</cdn>
    
    <!-- report theme --> <!-- standard, dark -->
    <theme>standard</theme>
  
    <!-- document encoding -->  <!-- defaults to UTF-8 -->
    <encoding>UTF-8</encoding>
    
    <!-- protocol for script and stylesheets -->   <!-- defaults to https -->
    <protocol>https</protocol>
    
    <!-- offline report -->
    <enableOfflineMode>false</enableOfflineMode>
		
    <!-- enable or disable timeline on dashboard -->
    <enableTimeline>true</enableTimeline>
        
    <!-- title of the document -->
    <documentTitle>ToolsQA - AllWinRobot2</documentTitle>
    
    <!-- report name - displayed at top-nav -->
    <reportName>ToolsQA - AllWinRobot2 Report</reportName>
    
    <!-- global date format override -->  <!-- defaults to yyyy-MM-dd -->
    <dateFormat>yyyy-MM-dd</dateFormat>
    
    <!-- global time format override -->   <!-- defaults to HH:mm:ss -->
    <timeFormat>HH:mm:ss</timeFormat>
    
    <!-- settings to enable/disable views -->
	<enableCategoryView>true</enableCategoryView>
	<enableAuthorView>true</enableAuthorView>
	<enableExceptionView>true</enableExceptionView>
	<enableTestRunnerLogsView>true</enableTestRunnerLogsView>
	
    <!-- custom javascript ，下面的js是修改了logo-->
    <scripts>
      <![CDATA[
        $(document).ready(function() {
        	var pathName = document.location.pathname;
			var index = pathName.substr(1).indexOf("target");
			var homepath = pathName.substr(0,index+1);
			var father = document.querySelector('.nav-wrapper');
			var oldchild = document.querySelector('.brand-logo');
			var newchild = document.createElement("img"); 
			father.insertBefore(newchild,oldchild); 
			newchild.setAttribute('src',homepath + "src/main/resources/reportlogo/allwin.png");
			newchild.style.width=86+"px";
			newchild.style.height=45+"px";
			father.removeChild(oldchild);
        });
      ]]>
    </scripts>
    
    <!-- custom styles -->
    <styles>
      <![CDATA[
        
      ]]>
    </styles>
  </configuration>
</extentreports>